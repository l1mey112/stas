#include "std.stas"

uput_std ! in num do
	local buf [20]
	local pos 0
	local len 0

	if num 0 = do
		buf 48 & :: 8 ; `0`
		buf 1 write
		return
	end

	buf 20 += pop pos

	num
	while @ 0 > do
		pos -- pop pos

		10 %% ; get modulo
		48 += ; convert to ascii
		pos ~ & :: 8

		len ++ pop len
	end
	_
	pos len write
end

main ! in do
	19952 uput_std endl
end